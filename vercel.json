{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
        { "src": "/api/createUser", "dest": "backend/Routes/CreateUser.js" },
    { "src": "/api/displayData", "dest": "backend/Routes/DisplayData.js" },
    { "src": "/(.*)", "dest": "backend/index.js" }
    ]
  }
  